# Definiciones de alias
alias cd..='cd ..'
alias cd...='cd ../..'
alias ..='cd ..'
alias ...='cd ../..'
alias nnn='nnn -o'
alias cal="ncal -Mb"
alias hrb="hr ' '"
alias hr2="hr ' ' - ' '"
alias goto='. gotoaux'
alias b='. b $@'

#Evitando errores de tecleo
alias car='cat'

#ejemplo: pwd | c --> al portapapeles
alias c='xclip -sel clip'
alias p='xclip -o -sel clip'

